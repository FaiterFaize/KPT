public class some {
    public static boolean abcmath (int a, int b, int c) {
        boolean exit = true;
        for (int i = 0; i < b; i++) {
            a += a;
        }
        return a % c == 0;
    }
    public static void main (String[] args) {
        System.out.println(abcmath(1, 2, 3));
    }
}
